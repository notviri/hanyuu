{
    "name": "hanyuu",
    "version": "1.0.0",
    "description": "Cute & minimal utility bot for Discord.",
    "repository": "https://github.com/notviri/hanyuu.git",
    "author": "viri <hi@viri.moe>",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "build": "tsc",
        "launch": "node build/main.js"
    },
    "dependencies": {
        "@discordjs/opus": "^0.3.2",
        "@types/node": "^14.6.4",
        "bufferutil": "^4.0.1",
        "eris": "^0.13.3",
        "erlpack": "abalabahaha/erlpack",
        "sodium-native": "^3.2.0",
        "zlib-sync": "^0.1.7"
    },
    "prettier": {
        "printWidth": 80,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": false,
        "quoteProps": "consistent",
        "trailingComma": "all",
        "bracketSpacing": true,
        "arrowParens": "always",
        "endOfLine": "lf"
    },
    "hanyuu": {
        "config": "config.json"
    }
